{
    "name": "rcknr/nova-multiselect-filter",
    "description": "A Laravel Nova filter that allows multiple selection.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergii Kauk",
            "email": "sergii@kauk.at",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "rcknr\\Nova\\Filters\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rcknr\\Nova\\Filters\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
